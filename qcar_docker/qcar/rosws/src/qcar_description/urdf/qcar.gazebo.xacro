<?xml version="1.0"?>
<robot name="QCar" xmlns:xacro="http://www.ros.org/wiki/xacro">
<!-- ROS 2 Gazebo Plugins for QCar -->

  <!-- Base link must have inertia for Gazebo -->
  <gazebo reference="base">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- RGB Camera (Intel RealSense) - ROS 2 -->
  <gazebo reference="camera_rgb">
    <sensor type="camera" name="intelrealsense_rgb">
      <pose>0 0 0 0 0 0</pose>
      <update_rate>30.0</update_rate>
      <camera name="rgb">
        <horizontal_fov>1.21126</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        <ros>
          <namespace>qcar</namespace>
          <remapping>image_raw:=rgb/image_color</remapping>
          <remapping>camera_info:=rgb/camera_info</remapping>
        </ros>
        <camera_name>rgb</camera_name>
        <frame_name>camera_rgb</frame_name>
        <hack_baseline>0.07</hack_baseline>
      </plugin>
    </sensor>
  </gazebo>

  <!-- CSI Front Camera - ROS 2 -->
  <gazebo reference="camera_csi_front">
    <sensor type="camera" name="csi_front">
      <pose>0 0 0 0 0 0</pose>
      <update_rate>30.0</update_rate>
      <camera name="front">
        <horizontal_fov>2.79253</horizontal_fov>
        <image>
          <width>1280</width>
          <height>640</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
        <distortion>
          <k1>-0.264598808</k1>
          <k2>0.0156281135</k2>
          <k3>0.0822019378</k3>
          <p1>0.0000652954</p1>
          <p2>0.0053984313</p2>
        </distortion>
      </camera>
      <plugin name="camera_csi_front_controller" filename="libgazebo_ros_camera.so">
        <ros>
          <namespace>qcar</namespace>
          <remapping>image_raw:=csi_front/image_raw</remapping>
          <remapping>camera_info:=csi_front/camera_info</remapping>
        </ros>
        <camera_name>csi_front</camera_name>
        <frame_name>camera_csi_front</frame_name>
      </plugin>
    </sensor>
  </gazebo>

